		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.20.5",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.3.15",
		"clsx": "^2.1.1",
		"concurrently": "^9.1.2",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
